{
  "name": "chessfield",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "types": "chessfield.d.ts",
  "files": [
    "dist"
  ],
  "main": "./dist/chessfield.umd.cjs",
  "module": "./dist/chessfield.js",
  "exports": {
    ".": {
      "import": "./dist/chessfield.js",
      "require": "./dist/chessfield.umd.cjs"
    },
    "./style.css": "./dist/chessfield.css"
  },
  "scripts": {
    "prepare": "$npm_execpath run compile",
    "compile": "tsc --sourceMap --declaration",
    "lint": "eslint src/*.ts",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "bundle": "vite build",
    "dist": "$npm_execpath run compile && $npm_execpath run bundle",
    "dev": "vite",
    "preview": "vite preview"
  },
  "dependencies": {
    "@chess-fu/fen-parser": "^1.1.4",
    "@types/three": "^0.173.0",
    "chessground": "^9.1.1",
    "eslint": "^9.21.0",
    "prettier": "^3.5.2",
    "rxjs": "^7.8.2",
    "three": "^0.170.0",
    "typescript": "~5.7.3",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "globals": "^16.0.0"
  }
}
